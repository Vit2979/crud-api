    "scripts": {
      "start:prod": "npm-run-all --parallel watch:server watch:build:prod",
      "start:dev": "npm-run-all --parallel watch:server watch:build:dev",
      "start:multi": "npm-run-all --parallel watch:multi watch:build:prod",
      "watch:server": "nodemon ./app/main.js",
      "watch:multi": "nodemon ./app/main.js multi",
      "watch:build:prod": "webpack --mode production --watch",
      "watch:build:dev": "webpack --mode development --watch",
      "start": "node ./app/main.js",
      "build": "webpack --mode production",
